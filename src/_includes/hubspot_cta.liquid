{%- if site.lang == 'es' -%}
  <span class="{{ include.class }}" id="hs-cta-wrapper-{{ include.hubspot-es-id }}">
    <span class="hs-cta-node hs-cta-{{ include.hubspot-es-id }}" id="hs-cta-{{ include.hubspot-es-id }}">
      <!--[if lte IE 8]><div id="hs-cta-ie-element"></div><![endif]-->
      <a href="https://cta-redirect.hubspot.com/cta/redirect/3042464/{{ include.hubspot-es-id }}" ></a>
    </span>
    <script charset="utf-8" src="https://js.hscta.net/cta/current.js"></script>
    <script type="text/javascript"> hbspt.cta.load(3042464, '{{ include.hubspot-es-id }}', {}); </script>
  </span>
{%- else -%}
